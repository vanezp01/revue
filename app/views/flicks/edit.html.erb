<div class="col-md-10 col-md-offset-1">
	<div class="new-play-form">
		<h1> Fix your submitted flicks</h1>

		<%= simple_form_for @flick, html: {multipart: true} do |f| %>
			<%= f.file_field :flick_img %>
			<%= f.select :category_id, @categories %>
		  <%= f.input :title %>
		  <%= f.input :description %>
		  <%= f.input :director %>
		  <%= f.button :submit %>
		<% end %>
	</div>
</div>
